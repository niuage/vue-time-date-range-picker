{
  "name": "vue-time-date-range-picker",
  "version": "1.4.0",
  "description": "a vue time date range picker",
  "keywords": [
    "vue",
    "vuejs",
    "range-datetime-picker",
    "date-time-range-picker"
  ],
  "main": "/dist/vdprDatePicker.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack",
    "preserve": "rimraf demo",
    "serve": "webpack-dev-server --config webpack-serve.config.js --open",
    "lint": "eslint --fix --ext .js,.vue src example test/unit/specs",
    "test": "jest --config test/unit/jest.config.js --coverage"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limbara/vue-time-date-range-picker.git"
  },
  "files": [
    "src",
    "dist"
  ],
  "author": "Nico Limbara <nick_dev@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/limbara/vue-time-date-range-picker/issues"
  },
  "homepage": "https://github.com/limbara/vue-time-date-range-picker#readme",
  "private": false,
  "jest": {
    "coverageDirectory": "./test/unit/coverage/",
    "collectCoverage": true
  },
  "peerDependencies": {
    "moment": "^2.27.0",
    "vue": "^3.1.2"
  }
